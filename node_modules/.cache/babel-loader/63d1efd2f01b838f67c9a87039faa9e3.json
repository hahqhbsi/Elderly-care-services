plate>\n\n<script>\n    export default {\n        data() {\n            return {\n                dialogVisible: false,\n                ruleForm: {},\n                user: {},\n                heads: {\"headLogoutFontHoverColor\":\"#fff\",\"headFontSize\":\"20px\",\"headUserInfoFontColor\":\"#000\",\"headBoxShadow\":\"0 2px 17px 2px #000\",\"headTitleImgHeight\":\"44px\",\"headLogoutFontHoverBgColor\":\"#000\",\"headFontColor\":\"rgba(47, 50, 72, 1)\",\"headTitleImg\":false,\"headHeight\":\"60px\",\"headTitleImgBorderRadius\":\"22px\",\"headTitleImgUrl\":\"http://codegen.caihongy.cn/20210223/c1223b6716cc43669b76cbffddc9c171.png\",\"headBgColor\":\"#fff\",\"headTitleImgBoxShadow\":\"0 1px 6px #444\",\"headLogoutFontColor\":\"#000\",\"headUserInfoFontSize\":\"16px\",\"headTitleImgWidth\":\"44px\",\"headTitleStyle\":\"1\",\"headLogoutFontSize\":\"16px\"},\n            };\n        },\n        created() {\n            this.setHeaderStyle()\n        },\n        mounted() {\n            let sessionTable = this.$storage.get(\"sessionTable\")\n            this.$http({\n                url: sessionTable + '/session',\n                method: \"get\"\n            }).then(({\n                         data\n                     }) => {\n                if (data && data.code === 0) {\n                    this.user = data.data;\n                } else {\n                    let message = this.$message\n                    message.error(data.msg);\n                }\n            });\n        },\n        methods: {\n            onLogout() {\n                let storage = this.$storage\n                let router = this.$router\n                storage.clear()\n                router.replace({\n                    name: \"login\"\n                });\n            },\n            onIndexTap(){\n                window.location.href = `${this.$base.indexUrl}`\n            },\n            setHeaderStyle() {\n                this.$nextTick(()=>{\n                    document.querySelect